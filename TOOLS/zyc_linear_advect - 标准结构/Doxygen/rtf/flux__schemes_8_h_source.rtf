00074 {\cf17 inline} real_t whf_TcnsN_A(std::array<real_t, 5> q)\par
00075 \{\par
00076     global_string = {\cf22 "whf_TcnsN_A"};\par
00077     {\cf20 // 局部光滑因子β}\par
00078     std::array<real_t, 3> beta;\par
00079     beta[0] = 1.0/1.0 * powq(1.0 * q[0] - 2.0 * q[1] + 1.0 * q[2], 2) +\par
00080               1.0/4.0 * powq(1.0 * q[0] - 4.0 * q[1] + 3.0 * q[2], 2);\par
00081     beta[1] = 1.0/1.0 * powq(1.0 * q[1] - 2.0 * q[2] + 1.0 * q[3], 2) + \par
00082               1.0/4.0 * powq(1.0 * q[1] + 0.0 * q[2] - 1.0 * q[3], 2);\par
00083     beta[2] = 1.0/1.0 * powq(1.0 * q[2] - 2.0 * q[3] + 1.0 * q[4], 2) + \par
00084               1.0/4.0 * powq(3.0 * q[2] - 4.0 * q[3] + 1.0 * q[4], 2);\par
00085     {\cf20 // 全局光滑因子τ}\par
00086     real_t tau = std::abs(beta[2] - beta[0]);\par
00087 \par
00088     {\cf20 // 计算自适应阈值CT_A}\par
00089     real_t alpha1 = 10.0, alpha2 = 5.0;  {\cf20 //自适应参数}\par
00090     real_t xi = 1e-3, C_r = 0.24;        {\cf20 //小参数和切换参数}\par
00091     real_t epsilon_A = (0.9*C_r)/(1.0 - 0.9*C_r)*xi*xi;  {\cf20 //自适应参数ε_A}\par
00092 \par
00093     {\cf20 // 计算差分值}\par
00094     std::array<real_t, 4> delta_q;\par
00095     delta_q[0] = q[0] - q[1];\par
00096     delta_q[1] = q[1] - q[2];\par
00097     delta_q[2] = q[2] - q[3];\par
00098     delta_q[3] = q[3] - q[4];\par
00099 \par
00100     {\cf20 // 计算η值（用于检测解的局部特征）}\par
00101     std::array<real_t, 3> eta;\par
00102     eta[0] = (std::abs(2.0 * delta_q[0] * delta_q[1]) + epsilon_A)\par
00103            / (delta_q[0] * delta_q[0] + delta_q[1] * delta_q[1] + epsilon_A);\par
00104     eta[1] = (std::abs(2.0 * delta_q[1] * delta_q[2]) + epsilon_A)\par
00105            / (delta_q[1] * delta_q[1] + delta_q[2] * delta_q[2] + epsilon_A);\par
00106     eta[2] = (std::abs(2.0 * delta_q[2] * delta_q[3]) + epsilon_A)\par
00107            / (delta_q[2] * delta_q[2] + delta_q[3] * delta_q[3] + epsilon_A);\par
00108     \par
00109     {\cf20 // 计算η_min值（最小的η值）}\par
00110     real_t eta_min = std::min(\{eta[0],eta[1],eta[2]\});\par
00111 \par
00112     {\cf20 // 计算自适应函数参数}\par
00113     real_t m = 1.0 - std::min(1.0Q, eta_min/C_r);\par
00114     real_t g_m = (1 - m)*(1 - m)*(1 - m)*(1 - m)*(1 + 4*m);\par
00115 \par
00116     {\cf20 // 计算beta_A参数}\par
00117     {\cf18 int} beta_A = (int)floorq(alpha1 - alpha2*(1 - g_m));\par
00118 \par
00119     {\cf20 // 自适应阈值参数CT_A}\par
00120     real_t CT_A;\par
00121 \par
00122     {\cf20 // 求光滑度量gamma（用于构造截止函数）}\par
00123     {\cf17 const} real_t C = 1.0;\par
00124     {\cf17 const} real_t eps = 1e-40;\par
00125 \par
00126     std::array<real_t, 3> gamma;\par
00127 \par
00128     gamma[0] = C + tau/(beta[0] + eps);\par
00129     gamma[1] = C + tau/(beta[1] + eps);\par
00130     gamma[2] = C + tau/(beta[2] + eps);\par
00131 \par
00132     {\cf20 // 对gamma进行幂运算处理}\par
00133     gamma[0] = powq(gamma[0], 2);\par
00134     gamma[1] = powq(gamma[1], 2);\par
00135     gamma[2] = powq(gamma[2], 2);\par
00136 \par
00137     gamma[0] = powq(gamma[0], 3);\par
00138     gamma[1] = powq(gamma[1], 3);\par
00139     gamma[2] = powq(gamma[2], 3);\par
00140 \par
00141     {\cf20 // 计算gamma总和}\par
00142     real_t gamma_sum = gamma[0] + gamma[1] + gamma[2];\par
00143     \par
00144     {\cf20 // 根据beta_A值确定CT_A}\par
00145     {\cf19 switch}(beta_A) \{\par
00146     {\cf19 case} 5: CT_A = 1e-5; \par
00147     {\cf19 break};\par
00148     {\cf19 case} 6: CT_A = 1e-6;\par
00149     {\cf19 break};\par
00150     {\cf19 case} 7: CT_A = 1e-7;\par
00151     {\cf19 break};\par
00152     {\cf19 case} 8: CT_A = 1e-8;\par
00153     {\cf19 break};\par
00154     {\cf19 case} 9: CT_A = 1e-9;\par
00155     {\cf19 break};\par
00156     {\cf19 case} 10: CT_A = 1e-10;\par
00157     {\cf19 break};\par
00158     \}\par
00159     \par
00160     {\cf20 // 计算判定参数rr}\par
00161     real_t rr;\par
00162     rr = CT_A*gamma_sum;\par
00163     {\cf20 // 构造截止函数}\par
00164     {\cf18 unsigned} {\cf18 short} flag = 0;\par
00165     {\cf19 if} (gamma[0] < rr) flag += 1;\par
00166     {\cf19 if} (gamma[1] < rr) flag += 2;\par
00167     {\cf19 if} (gamma[2] < rr) flag += 4;\par
00168 \par
00169     {\cf20 // 根据flag值进行插值}\par
00170     {\cf19 switch} (flag) \{\par
00171     {\cf19 case} 0:  {\cf20 // 111}\par
00172         {\cf19 return} 3.0/128.0 * q[0] - 5.0/32.0 * q[1] + 45.0/64.0 * q[2] + 15.0/32.0 * q[3] - 5.0/128.0 * q[4];\par
00173     {\cf19 case} 1:  {\cf20 // 011}\par
00174         {\cf19 return} -1.0/16.0 * q[1] + 9.0/16.0 * q[2] + 9.0/16.0 * q[3] - 1.0/16.0 * q[4];\par
00175     {\cf19 case} 2:  {\cf20 // 101}\par
00176     {\cf19 case} 3:  {\cf20 // 001}\par
00177         {\cf19 return} 3.0/8.0 * q[2] + 3.0/4.0 * q[3] - 1.0/8.0 * q[4];\par
00178     {\cf19 case} 4:  {\cf20 // 110}\par
00179         {\cf19 return} 1.0/16.0 * q[0] - 5.0/16.0 * q[1] + 15.0/16.0 * q[2] + 5.0/16.0 * q[3];\par
00180     {\cf19 case} 5:  {\cf20 // 010}\par
00181         {\cf19 return} -1.0/8.0 * q[1] + 3.0/4.0 * q[2] + 3.0/8.0 * q[3];\par
00182     {\cf19 case} 6:  {\cf20 // 100}\par
00183         {\cf19 return} 3.0/8.0 * q[0] - 5.0/4.0 * q[1] + 15.0/8.0 * q[2];\par
00184     {\cf19 default}: {\cf20 // 000: 强间断区(直接取值)}\par
00185         {\cf19 return} q[2];\par
00186     \}\par
00187 \}\par
