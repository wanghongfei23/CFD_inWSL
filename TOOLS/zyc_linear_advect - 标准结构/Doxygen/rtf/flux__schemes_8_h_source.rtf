00190 {\cf17 inline} real_t whf_TcnsN_myASF203_NoS(std::array<real_t, 5> q) \par
00191 \{\par
00192     global_string = {\cf22 "whf_TcnsN_myASF203_NoS"};\par
00193 \par
00194     {\cf20 // 常量定义}\par
00195     {\cf17 constexpr} real_t xi = 1e-3;           {\cf20 // 小参数}\par
00196     {\cf17 constexpr} real_t Cr = 0.24;           {\cf20 // 切换参数}\par
00197     {\cf17 constexpr} real_t C = 1.0;             {\cf20 // 常数}\par
00198     {\cf17 constexpr} real_t epsilon_A = (0.9 * Cr) / (1 - 0.9 * Cr) * xi * xi;  {\cf20 // 自适应参数ε_A}\par
00199 \par
00200     {\cf20 // 解包输入值}\par
00201     real_t q_im2 = q[0]; {\cf20 // q_\{i-2\}}\par
00202     real_t q_im1 = q[1]; {\cf20 // q_\{i-1\}}\par
00203     real_t q_i   = q[2]; {\cf20 // q_i}\par
00204     real_t q_ip1 = q[3]; {\cf20 // q_\{i+1\}}\par
00205     real_t q_ip2 = q[4]; {\cf20 // q_\{i+2\}}\par
00206 \par
00207     {\cf20 // 计算差分值}\par
00208     real_t dq_im3_2 = q_im1 - q_im2; {\cf20 // Δq_\{i-3/2\}}\par
00209     real_t dq_im1_2 = q_i - q_im1;   {\cf20 // Δq_\{i-1/2\}}\par
00210     real_t dq_ip1_2 = q_ip1 - q_i;   {\cf20 // Δq_\{i+1/2\}}\par
00211     real_t dq_ip3_2 = q_ip2 - q_ip1; {\cf20 // Δq_\{i+3/2\}}\par
00212 \par
00213     {\cf20 // 计算局部光滑因子}\par
00214     real_t beta0 = powq(q_im2 - 2*q_im1 + q_i, 2) + \par
00215                  0.25 * powq(q_im2 - 4*q_im1 + 3*q_i, 2);\par
00216     \par
00217     real_t beta1 = powq(q_im1 - 2*q_i + q_ip1, 2) + \par
00218                  0.25 * powq(q_im1 - q_ip1, 2);\par
00219     \par
00220     real_t beta2 = powq(q_i - 2*q_ip1 + q_ip2, 2) + \par
00221                  0.25 * powq(3*q_i - 4*q_ip1 + q_ip2, 2);\par
00222 \par
00223     {\cf20 // 计算全局光滑因子}\par
00224     real_t tau = std::abs(beta2 - beta0);\par
00225 \par
00226     {\cf20 // 计算η值}\par
00227     real_t eta_im1 = (std::abs(2*dq_im1_2*dq_im3_2) + epsilon_A) / \par
00228                    (powq(dq_im1_2, 2) + powq(dq_im3_2, 2) + epsilon_A);\par
00229     \par
00230     real_t eta_i = (std::abs(2*dq_ip1_2*dq_im1_2) + epsilon_A) / \par
00231                  (powq(dq_ip1_2, 2) + powq(dq_im1_2, 2) + epsilon_A);\par
00232     \par
00233     real_t eta_ip1 = (std::abs(2*dq_ip3_2*dq_ip1_2) + epsilon_A) / \par
00234                    (powq(dq_ip3_2, 2) + powq(dq_ip1_2, 2) + epsilon_A);\par
00235     \par
00236     real_t eta_min = std::min(\{eta_im1, eta_i, eta_ip1\});\par
00237 \par
00238     {\cf20 // 计算m参数}\par
00239     real_t min = std::min(1.0Q, eta_min / Cr);\par
00240     real_t m = 1.0Q - min;\par
00241 \par
00242     {\cf20 // 计算C_T_prime（自适应阈值参数）}\par
00243     real_t C_T_prime = -0.452662*m*m*m + 0.590811*m*m - 0.012189*m + 0.022010;\par
00244 \par
00245     {\cf20 // 确定β_j}\par
00246     real_t beta_j = std::min(\{beta0, beta1, beta2\});\par
00247 \par
00248     {\cf20 // 计算δ_k}\par
00249     {\cf18 int} delta0 = (beta_j * tau < (C_T_prime * tau - C * (1 - C_T_prime) * beta_j) * beta0) ? 0 : 1;\par
00250     {\cf18 int} delta1 = (beta_j * tau < (C_T_prime * tau - C * (1 - C_T_prime) * beta_j) * beta1) ? 0 : 1;\par
00251     {\cf18 int} delta2 = (beta_j * tau < (C_T_prime * tau - C * (1 - C_T_prime) * beta_j) * beta2) ? 0 : 1;\par
00252 \par
00253     {\cf20 // 根据δ值选择插值系数}\par
00254     real_t a0, a1, a2, a3, a4;\par
00255     \par
00256     {\cf19 if} (delta0 == 1 && delta1 == 1 && delta2 == 1) \{\par
00257         a0 = 3.0/128; a1 = -5.0/32; a2 = 45.0/64; a3 = 15.0/32; a4 = -5.0/128;\par
00258     \} \par
00259     {\cf19 else} {\cf19 if} (delta0 == 0 && delta1 == 1 && delta2 == 1) \{\par
00260         a0 = 0; a1 = -1.0/16; a2 = 9.0/16; a3 = 9.0/16; a4 = -1.0/16;\par
00261     \} \par
00262     {\cf19 else} {\cf19 if} ((delta0 == 1 && delta1 == 0 && delta2 == 1) || \par
00263              (delta0 == 0 && delta1 == 0 && delta2 == 1)) \{\par
00264         a0 = 0; a1 = 0; a2 = 3.0/8; a3 = 3.0/4; a4 = -1.0/8;\par
00265     \} \par
00266     {\cf19 else} {\cf19 if} (delta0 == 1 && delta1 == 1 && delta2 == 0) \{\par
00267         a0 = 1.0/16; a1 = -5.0/16; a2 = 15.0/16; a3 = 5.0/16; a4 = 0;\par
00268     \} \par
00269     {\cf19 else} {\cf19 if} (delta0 == 0 && delta1 == 1 && delta2 == 0) \{\par
00270         a0 = 0; a1 = -1.0/8; a2 = 3.0/4; a3 = 3.0/8; a4 = 0;\par
00271     \} \par
00272     {\cf19 else} {\cf19 if} (delta0 == 1 && delta1 == 0 && delta2 == 0) \{\par
00273         a0 = 3.0/8; a1 = -5.0/4; a2 = 15.0/8; a3 = 0; a4 = 0;\par
00274     \} \par
00275     {\cf19 else} \{ {\cf20 // (0,0,0)}\par
00276         a0 = 0; a1 = 0; a2 = 1; a3 = 0; a4 = 0;\par
00277     \}\par
00278 \par
00279     {\cf20 // 计算最终插值结果}\par
00280     {\cf19 return} a0*q_im2 + a1*q_im1 + a2*q_i + a3*q_ip1 + a4*q_ip2;\par
00281 \}\par
