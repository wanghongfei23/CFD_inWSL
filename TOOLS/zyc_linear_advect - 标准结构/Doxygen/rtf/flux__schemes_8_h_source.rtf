00010 {\cf20 // 定义高精度浮点类型real_t为__float128}\par
00011 {\cf17 typedef} __float128 real_t;\par
00012 \par
00013 {\cf17 inline} real_t Teno5_CongZ({\cf17 const} std::array<real_t, 5>& q)\par
00014 \{\par
00015     global_string = {\cf22 "Teno5_CongZ"};\par
00016     {\cf20 // real_t eps = 1.0e-40Q;}\par
00017     {\cf20 // 计算三个子模板的光滑指示子β}\par
00018     std::array<real_t, 3> beta = \{\par
00019         1.0Q / 1.0Q * powq(1.0Q * q[0] - 2.0Q * q[1] + 1.0Q * q[2], 2) + 1.0Q / 4.0Q * powq(1.0Q * q[0] - 4.0Q * q[1] + 3.0Q * q[2], 2),\par
00020         1.0Q / 1.0Q * powq(1.0Q * q[1] - 2.0Q * q[2] + 1.0Q * q[3], 2) + 1.0Q / 4.0Q * powq(1.0Q * q[1] + 0.0Q * q[2] - 1.0Q * q[3], 2),\par
00021         1.0Q / 1.0Q * powq(1.0Q * q[2] - 2.0Q * q[3] + 1.0Q * q[4], 2) + 1.0Q / 4.0Q * powq(3.0Q * q[2] - 4.0Q * q[3] + 1.0Q * q[4], 2)\par
00022     \};\par
00023     {\cf20 // 找到最小的光滑指示子索引}\par
00024     {\cf18 unsigned} {\cf18 short} minBeta = std::min_element(beta.begin(), beta.end()) - beta.begin();\par
00025     {\cf20 // constexpr real_t CT = powq(1.5 * 1.0e-5, 1.0 / 6.0);}\par
00026     {\cf20 // constexpr real_t CT_1 = 1.0 - CT;}\par
00027     {\cf20 // 计算阈值参数}\par
00028     {\cf17 const} real_t CT = powq(1.5 * 1.0e-5, 1.0 / 6.0);\par
00029     {\cf17 const} real_t CT_1 = 1.0 - CT;\par
00030     {\cf20 // 计算全局光滑指示子τ}\par
00031     real_t tau = std::abs(beta[2] - beta[0]);\par
00032     {\cf20 // 计算判定参数}\par
00033     real_t rr = CT * tau - CT_1 * beta[minBeta];\par
00034     real_t ll = tau * beta[minBeta];\par
00035     {\cf20 // 根据光滑性判断是否使用模板}\par
00036     {\cf18 unsigned} {\cf18 short} flag = 0;\par
00037     {\cf19 if} (ll < rr * beta[0])\par
00038         flag += 1;\par
00039     {\cf19 if} (ll < rr * beta[1])\par
00040         flag += 2;\par
00041     {\cf19 if} (ll < rr * beta[2])\par
00042         flag += 4;\par
00043     {\cf20 // 根据flag值选择不同的重构系数}\par
00044     {\cf19 switch} (flag) \{\par
00045     {\cf20 // switch (0) \{}\par
00046     {\cf19 case} 0:\par
00047         {\cf20 /* 1,1,1 */}\par
00048         {\cf19 return} 3.0Q / 128.0Q * q[0] - 5.0Q / 32.0Q * q[1] + 45.0Q / 64.0Q * q[2] + 15.0Q / 32.0Q * q[3] - 5.0Q / 128.0Q * q[4];\par
00049     {\cf19 case} 1:\par
00050         {\cf20 /* 0,1,1 */}\par
00051         {\cf19 return} -1.0Q / 16.0Q * q[1] + 9.0Q / 16.0Q * q[2] + 9.0Q / 16.0Q * q[3] - 1.0Q / 16.0Q * q[4];\par
00052     {\cf19 case} 2:\par
00053         {\cf20 /* 1,0,1 */}\par
00054         {\cf19 return} 3.0Q / 8.0Q * q[2] + 3.0Q / 4.0Q * q[3] - 1.0Q / 8.0Q * q[4];\par
00055     {\cf19 case} 3:\par
00056         {\cf20 /* 0,0,1 */}\par
00057         {\cf19 return} 3.0Q / 8.0Q * q[2] + 3.0Q / 4.0Q * q[3] - 1.0Q / 8.0Q * q[4];\par
00058     {\cf19 case} 4:\par
00059         {\cf20 /* 1,1,0 */}\par
00060         {\cf19 return} 1.0Q / 16.0Q * q[0] - 5.0Q / 16.0Q * q[1] + 15.0Q / 16.0Q * q[2] + 5.0Q / 16.0Q * q[3];\par
00061     {\cf19 case} 5:\par
00062         {\cf20 /* 0,1,0 */}\par
00063         {\cf19 return} -1.0Q / 8.0Q * q[1] + 3.0Q / 4.0Q * q[2] + 3.0Q / 8.0Q * q[3];\par
00064     {\cf19 case} 6:\par
00065         {\cf20 /* 1,0,0 */}\par
00066         {\cf19 return} 3.0Q / 8.0Q * q[0] - 5.0Q / 4.0Q * q[1] + 15.0Q / 8.0Q * q[2];\par
00067     {\cf19 default}:\par
00068         {\cf20 /* 0,0,0 */}\par
00069         {\cf19 return} q[2];\par
00070     \}\par
00071 \}\par
