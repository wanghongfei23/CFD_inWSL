00501 {\cf18 void} evolve(GhostArray& u, GhostArray& u_prev, GhostArray& flux, GhostArray& rhs,\par
00502     real_t dt, real_t dx) {\cf20 // 添加dx参数}\par
00503 \{\par
00504     {\cf20 // 阶段1}\par
00505     compute_rhs(rhs, u_prev, flux, dx); {\cf20 // 现在dx已定义}\par
00506 {\cf20 // #pragma omp parallel for}\par
00507     {\cf19 for} ({\cf18 int} i : u.domain()) \{\par
00508         u[i] = u_prev[i] + dt * rhs[i];\par
00509     \}\par
00510 \par
00511     {\cf20 // 阶段2}\par
00512     GhostArray temp(u);\par
00513     compute_rhs(rhs, u, flux, dx); {\cf20 // 传递dx}\par
00514 {\cf20 // #pragma omp parallel for}\par
00515     {\cf19 for} ({\cf18 int} i : u.domain()) \{\par
00516         temp[i] = 0.75Q * u_prev[i] + 0.25Q * (u[i] + dt * rhs[i]);\par
00517     \}\par
00518 \par
00519     {\cf20 // 阶段3}\par
00520     compute_rhs(rhs, temp, flux, dx); {\cf20 // 传递dx}\par
00521 {\cf20 // #pragma omp parallel for}\par
00522     {\cf19 for} ({\cf18 int} i : u.domain()) \{\par
00523         u[i] = (1.0Q / 3.0Q) * u_prev[i] + (2.0Q / 3.0Q) * (temp[i] + dt * rhs[i]);\par
00524     \}\par
00525 \}\par
