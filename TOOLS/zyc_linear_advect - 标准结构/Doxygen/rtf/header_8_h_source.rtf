00434 {\cf18 void} compute_rhs(GhostArray& rhs, GhostArray& u, GhostArray& flux, real_t dx)\par
00435 \{\par
00436     {\cf17 const} real_t c = 1.0Q; {\cf20 // 波速}\par
00437     {\cf19 if} (is_periodic)\par
00438         u.fill_periodic();\par
00439 \par
00440     {\cf20 // 1. 一阶插值计算界面通量 (i+1/2位置)}\par
00441 {\cf20 // #pragma omp parallel for}\par
00442     {\cf19 for} ({\cf18 int} i : flux.full_range()) \{\par
00443         {\cf20 // 提取左值和右值}\par
00444         {\cf20 // real_t u_left = u[i - 1];}\par
00445         {\cf20 // real_t u_right = u[i];}\par
00446         std::array<real_t, 5> q_left = \{ u[i - 3], u[i - 2], u[i - 1], u[i], u[i + 1] \};\par
00447         std::array<real_t, 5> q_right = \{ u[i + 2], u[i + 1], u[i], u[i - 1], u[i - 2] \};\par
00448 \par
00449         {\cf20 // }\par
00450         {\cf20 // 使用高阶插值得到左值和右值}\par
00451         \par
00452         {\cf20 // 【王鸿飞】插值格式更换处}\par
00453         {\cf20 // real_t u_left = whf_TcnsN_A(q_left);}\par
00454         {\cf20 // real_t u_right = whf_TcnsN_A(q_right);}\par
00455         {\cf20 // real_t u_left = whf_TcnsN_myASF203_NoS(q_left);}\par
00456         {\cf20 // real_t u_right = whf_TcnsN_myASF203_NoS(q_right);}\par
00457         real_t u_left = Linear_5th(q_left);\par
00458         real_t u_right = Linear_5th(q_right);\par
00459 \par
00460         {\cf20 // 一阶插值:取左右平均值}\par
00461         {\cf20 // if (c > 0) \{}\par
00462         {\cf20 //     // c > 0, 使用前向差分}\par
00463         {\cf20 //     flux[i] = c * u_left;}\par
00464         {\cf20 // \} else \{}\par
00465         {\cf20 //     // c <= 0, 使用后向差分}\par
00466         {\cf20 //     flux[i] = c * u_right;}\par
00467         {\cf20 // \}}\par
00468 \par
00469         real_t F_left = c * u_left;\par
00470         real_t F_right = c * u_right;\par
00471         flux[i] = (F_left + F_right) / 2.0Q - (c / 2.0Q) * (u_right - u_left);\par
00472     \}\par
00473     {\cf19 if} (is_periodic)\par
00474         flux.fill_periodic();\par
00475     std::array<real_t, 3> w = \{ 3.0Q / 2.0Q, -3.0Q / 10.0Q, 1.0Q / 30.0Q \};\par
00476 {\cf20 // std::array<real_t, 3> w = \{ 4.0Q / 3.0Q, -1.0Q / 6.0Q, 0.0Q \};}\par
00477 {\cf20 // std::array<real_t, 3> w = \{ 1.0Q, 0.0Q, 0.0Q \};}\par
00478 {\cf20 // 2. 二阶中心差分计算右端项}\par
00479 {\cf20 // #pragma omp parallel for}\par
00480     {\cf19 for} ({\cf18 int} i : rhs.domain()) \{\par
00481         {\cf20 // 二阶差分: (flux[i+1] - flux[i]) / dx}\par
00482         {\cf20 // rhs[i] = -1.0Q / dx * (flux[i + 1] - flux[i]);}\par
00483         rhs[i] = -w[0] / dx * (flux[i + 1] - flux[i])\par
00484             - w[1] / dx * (u[i + 1] - u[i - 1])\par
00485             - w[2] / dx * (flux[i + 2] - flux[i - 1]);\par
00486     \}\par
00487 \}\par
