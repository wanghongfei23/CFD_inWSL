00757 {\cf20 /* Convert integer to decimal digit literals.  */}\par
00758 {\cf21 #define DEC(n)                   \\}\par
00759 {\cf21   ('0' + (((n) / 10000000)%10)), \\}\par
00760 {\cf21   ('0' + (((n) / 1000000)%10)),  \\}\par
00761 {\cf21   ('0' + (((n) / 100000)%10)),   \\}\par
00762 {\cf21   ('0' + (((n) / 10000)%10)),    \\}\par
00763 {\cf21   ('0' + (((n) / 1000)%10)),     \\}\par
00764 {\cf21   ('0' + (((n) / 100)%10)),      \\}\par
00765 {\cf21   ('0' + (((n) / 10)%10)),       \\}\par
00766 {\cf21   ('0' +  ((n) % 10))}\par
