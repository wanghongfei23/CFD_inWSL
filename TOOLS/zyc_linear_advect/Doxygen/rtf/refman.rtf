00081     {\cf20 // \'CD\'F8\'B8\'F1\'C3\'DC\'B6\'C8\'D0\'F2\'C1\'D0\'A3\'A8\'D3\'C3\'D3\'DA\'CA\'D5\'C1\'B2\'D0\'D4\'B7\'D6\'CE\'F6\'A3\'A9}\par
00082     std::vector<int> grid_densities = \{ 50, 100, 150, 200, 300, 400, 500 \};\par
00083     {\cf20 // std::vector<int> grid_densities = \{ 50, 100\};}\par
00084 \par
00085     {\cf20 // \'B2\'CE\'CA\'FD\'C9\'E8\'D6\'C3}\par
00086     {\cf17 const} {\cf18 int} ghost = 5;     {\cf20 // \'C3\'BF\'B2\'E0\'D0\'E9\'C4\'E2\'B5\'E3\'CA\'FD}\par
00087     {\cf17 const} real_t L = 1.0Q;   {\cf20 // \'BC\'C6\'CB\'E3\'D3\'F2\'B3\'A4\'B6\'C8}\par
00088     {\cf17 const} real_t T = 1.0Q;   {\cf20 // \'D7\'DC\'C4\'A3\'C4\'E2\'CA\'B1\'BC\'E4}\par
00089     {\cf17 const} real_t CFL = 0.01Q; {\cf20 // CFL\'CA\'FD}\par
00090 \par
00091     {\cf20 // \'A1\'BE\'CD\'F5\'BA\'E8\'B7\'C9\'A1\'BF}\par
00092     {\cf20 // \'BC\'C6\'CB\'E3\'C3\'BF\'B8\'F6\'CD\'F8\'B8\'F1\'C3\'DC\'B6\'C8\'B6\'D4\'D3\'A6\'B5\'C4\'CD\'F8\'B8\'F1\'B2\'BD\'B3\'A4}\par
00093     std::vector<real_t> Lpergrid_densities;\par
00094     {\cf19 for} ({\cf18 int} density : grid_densities) \par
00095     \{\par
00096         Lpergrid_densities.push_back(L / density);\par
00097     \}\par
00098     \par
00099     {\cf20 // \'B4\'E6\'B4\'A2\'C3\'BF\'D6\'D6\'CD\'F8\'B8\'F1\'C3\'DC\'B6\'C8\'CF\'C2\'B5\'C4\'CE\'F3\'B2\'EE\'CF\'F2\'C1\'BF}\par
00100     std::vector<real_t> L1_errors;\par
00101     std::vector<real_t> L2_errors;\par
00102     std::vector<real_t> Linf_errors;\par
00103 \par
00104     {\cf20 // \'B4\'B4\'BD\'A8\'BD\'E1\'B9\'FB\'CE\'C4\'BC\'FE}\par
00105     std::ofstream results_file({\cf22 "results.txt"});\par
00106     {\cf19 if} (!results_file.is_open()) \{\par
00107         std::cerr << {\cf22 "Failed to open results.txt for writing."} << std::endl;\par
00108         {\cf19 return} 1;\par
00109     \}\par
00110 \par
00111     {\cf20 // \'D0\'B4\'C8\'EB\'CE\'C4\'BC\'FE\'CD\'B7\'B2\'BF}\par
00112     results_file << {\cf22 "Variables = \\"dx\\", \\"L1_Error\\", \\"L1_Order\\", \\"L2_Error\\", \\"L2_Order\\", \\"Linf_Error\\", \\"Linf_Order\\"\\n"};\par
00113     results_file << {\cf22 "ZONE T=\\"Convergence Analysis\\"\\n"};\par
00114 \par
00115     {\cf20 // results_file << "N\\tL1_Error\\tL2_Error\\tLinf_Error\\tOrder_of_Accuracy\\n";}\par
00116 \par
00117     {\cf20 // \'B1\'E9\'C0\'FA\'CB\'F9\'D3\'D0\'CD\'F8\'B8\'F1\'C3\'DC\'B6\'C8\'BD\'F8\'D0\'D0\'BC\'C6\'CB\'E3}\par
00118     {\cf19 for} ({\cf18 int} N : grid_densities) \{\par
00119         {\cf20 // \'B4\'B4\'BD\'A8\'CD\'F8\'B8\'F1\'B1\'E4\'C1\'BF\'BC\'AF\'BA\'CF}\par
00120         GridVariables grid(N, ghost, L);\par
00121 \par
00122         {\cf20 // \'B3\'F5\'CA\'BC\'BB\'AF\'CD\'F8\'B8\'F1}\par
00123 {\cf20 // #pragma omp parallel for}\par
00124         {\cf19 for} (GhostArray::Range::Iterator i = grid.x.domain().begin(); i != grid.x.domain().end(); ++i) \{\par
00125             grid.x[*i] = (*i + 0.5Q) * grid.dx; {\cf20 // \'B5\'A5\'D4\'AA\'D6\'D0\'D0\'C4\'D0\'CD\'CD\'F8\'B8\'F1\'D7\'F8\'B1\'EA}\par
00126             {\cf20 // \'B7\'BD\'B2\'A8\'B3\'F5\'CA\'BC\'CC\'F5\'BC\'FE}\par
00127             grid.u[*i] = exact_initial_solution(grid.x[*i], 0.0);\par
00128         \}\par
00129         {\cf20 // \'CC\'EE\'B3\'E4\'D6\'DC\'C6\'DA\'B1\'DF\'BD\'E7\'CC\'F5\'BC\'FE}\par
00130         grid.x.fill_periodic();\par
00131         grid.u.fill_periodic();\par
00132 \par
00133         {\cf20 // \'CA\'B1\'BC\'E4\'CD\'C6\'BD\'F8\'D1\'AD\'BB\'B7}\par
00134         {\cf17 const} real_t dt = CFL * grid.dx; {\cf20 // \'BB\'F9\'B1\'BE\'CA\'B1\'BC\'E4\'B2\'BD\'B3\'A4}\par
00135         {\cf19 while} (grid.time < T) \{\par
00136             {\cf20 // \'BC\'C6\'CB\'E3\'D7\'D4\'CA\'CA\'D3\'A6\'CA\'B1\'BC\'E4\'B2\'BD\'B3\'A4}\par
00137             real_t current_dt = dt;\par
00138             {\cf19 if} (grid.time + dt > T) \{\par
00139                 current_dt = T - grid.time; {\cf20 // \'D7\'EE\'BA\'F3\'D2\'BB\'B2\'BD\'B5\'F7\'D5\'FB}\par
00140             \}\par
00141 \par
00142             {\cf20 // \'B1\'A3\'B4\'E6\'C7\'B0\'D2\'BB\'CA\'B1\'BC\'E4\'B2\'BD\'B5\'C4\'BD\'E2}\par
00143 {\cf20 // #pragma omp parallel for}\par
00144             {\cf19 for} (GhostArray::Range::Iterator i = grid.u.domain().begin(); i != grid.u.domain().end(); ++i) \{\par
00145                 grid.u_prev[*i] = grid.u[*i];\par
00146             \}\par
00147 \par
00148             {\cf20 // \'D1\'DD\'BB\'AF\'BD\'E2\'B5\'BD\'CF\'C2\'D2\'BB\'CA\'B1\'BC\'E4\'B2\'BD\'A3\'A8\'CA\'B9\'D3\'C3\'C8\'FD\'BD\'D7Runge-Kutta\'B7\'BD\'B7\'A8\'A3\'A9}\par
00149             evolve(grid.u, grid.u_prev, grid.flux, grid.rhs, current_dt, grid.dx);\par
00150 \par
00151             {\cf20 // \'B8\'FC\'D0\'C2\'B5\'B1\'C7\'B0\'CA\'B1\'BC\'E4}\par
00152             grid.time += current_dt;\par
00153         \}\par
00154         \par
00155         {\cf20 // \'BD\'AB\'BD\'E1\'B9\'FB\'D0\'B4\'C8\'EBTecplot\'CE\'C4\'BC\'FE}\par
00156         std::string filename = {\cf22 "output_N"} + std::to_string(N) + {\cf22 ".dat"};\par
00157         write_to_tecplot_file(grid, grid.time, filename);\par
00158 \par
00159         {\cf20 // \'BC\'C6\'CB\'E3\'B8\'F7\'D6\'D6\'B7\'B6\'CA\'FD\'CE\'F3\'B2\'EE}\par
00160         real_t L1_error = L1_norm_error(grid.u, grid.x, 0.0Q);\par
00161         real_t L2_error = L2_norm_error(grid.u, grid.x, 0.0Q);\par
00162         real_t Linf_error = Linf_norm_error(grid.u, grid.x, 0.0Q);\par
00163 \par
00164         {\cf20 // \'B4\'E6\'B4\'A2\'CE\'F3\'B2\'EE}\par
00165         L1_errors.push_back(L1_error);\par
00166         L2_errors.push_back(L2_error);\par
00167         Linf_errors.push_back(Linf_error);\par
00168 \par
00169         {\cf20 // \'CA\'E4\'B3\'F6\'CE\'F3\'B2\'EE\'D0\'C5\'CF\'A2}\par
00170         std::cout << {\cf22 "Grid density: "} << N << {\cf22 "\\n"};\par
00171         std::cout << {\cf22 "L1 norm error: "} << L1_error << {\cf22 "\\n"};\par
00172         std::cout << {\cf22 "L2 norm error: "} << L2_error << {\cf22 "\\n"};\par
00173         std::cout << {\cf22 "L-infinity norm error: "} << Linf_error << {\cf22 "\\n\\n"};\par
00174     \}\par
00175 \par
00176     {\cf20 // \'BC\'C6\'CB\'E3\'CA\'D5\'C1\'B2\'BD\'D7}\par
00177     std::cout << {\cf22 "Order of accuracy:\\n"};\par
00178     {\cf20 // results_file << grid_densities[0] << "\\t" << std::setprecision(5) << std::scientific }\par
00179     results_file << Lpergrid_densities[0] << {\cf22 "\\t"} << std::setprecision(5) << std::scientific \par
00180                  << L1_errors[0] << {\cf22 "\\t0\\t"} << L2_errors[0] << {\cf22 "\\t0\\t"} << Linf_errors[0] << {\cf22 "\\t0\\n"};\par
00181     {\cf19 for} ({\cf18 size_t} i = 1; i < grid_densities.size(); ++i) \{\par
00182         real_t h1 = 1.0Q / grid_densities[i - 1];\par
00183         real_t h2 = 1.0Q / grid_densities[i];\par
00184 \par
00185         {\cf20 // \'CA\'B9\'D3\'C3\'B9\'AB\'CA\'BD\'BC\'C6\'CB\'E3\'CA\'D5\'C1\'B2\'BD\'D7\'A3\'BAp = log(error1/error2) / log(h1/h2)}\par
00186         real_t L1_order = log2(L1_errors[i - 1] / L1_errors[i]) / log2(h1 / h2);\par
00187         real_t L2_order = log2(L2_errors[i - 1] / L2_errors[i]) / log2(h1 / h2);\par
00188         real_t Linf_order = log2(Linf_errors[i - 1] / Linf_errors[i]) / log2(h1 / h2);\par
00189 \par
00190         std::cout << {\cf22 "From N = "} << grid_densities[i - 1] << {\cf22 " to N = "} << grid_densities[i] << {\cf22 ":\\n"};\par
00191         std::cout << {\cf22 "L1 norm order: "} << L1_order << {\cf22 "\\n"};\par
00192         std::cout << {\cf22 "L2 norm order: "} << L2_order << {\cf22 "\\n"};\par
00193         std::cout << {\cf22 "L-infinity norm order: "} << Linf_order << {\cf22 "\\n\\n"};\par
00194 \par
00195         {\cf20 // \'B8\'FC\'D0\'C2\'CA\'D5\'C1\'B2\'BD\'D7\'B5\'BD\'BD\'E1\'B9\'FB\'CE\'C4\'BC\'FE}\par
00196         {\cf20 // results_file << grid_densities[i] << "\\t" << std::setprecision(5) << std::scientific}\par
00197         results_file << Lpergrid_densities[i] << {\cf22 "\\t"} << std::setprecision(5) << std::scientific\par
00198                      << L1_errors[i] << {\cf22 "\\t"} << L1_order << {\cf22 "\\t"}\par
00199                      << L2_errors[i] << {\cf22 "\\t"} << L2_order << {\cf22 "\\t"}\par
00200                      << Linf_errors[i] << {\cf22 "\\t"} << Linf_order << {\cf22 "\\n"};\par
00201     \}\par
00202     results_file.close();\par
00203 \par
00204     std::cout << {\cf22 "\'A1\'BEwith_time\'A1\'BF: "} << std::endl;\par
00205     std::cout << {\cf22 "\'CA\'B9\'D3\'C3\'B5\'C4\'B2\'E5\'D6\'B5\'B8\'F1\'CA\'BD\'CE\'AA: "} << global_string << std::endl;\par
00206     \par
00207     {\cf19 return} 0;\par
00208 \}\par
}
